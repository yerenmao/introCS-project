class Tools {
    function int mod(int x, int y) {
        var int i;
        let i = 1;
        while(x > (i*y)) {
            let i = i + 1;
        }
        return x - ((i-1) * y);
    }

    function boolean collide(int x1, int y1, int sx1, int sy1, int x2, int y2, int sx2, int sy2) {
        // test left, up, right, down
        if(((x1+sx1) < x2) | ((y1+sy1) < y2) | (x1 > (x2+sx2)) | (y1 > (y2+sy2))) {
            return false;
        }
        return true;
    }
}